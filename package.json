{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for react-redux projects with React-Storybook, Flow & Nightwatch",
  "author": "Shawn McKay <shawn.j.mckay@gmail.com>",
  "scripts": {
    "build": "node scripts/build.js",
    "build-storybook": "build-storybook -s public",
    "e2e": "nightwatch",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install && webdriver-manager update",
    "server": "nodemon server",
    "start": "HTTPS=true && node scripts/start.js",
    "storybook": "start-storybook -p 9009",
    "storybook-build": "build-storybook -o ./.out",
    "test": "node scripts/test.js --env=jsdom"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": "react-app",
    "globals": {
      "resolveOwn": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true
      }
    },
    "rules": {
      "prefer-template": 0,
      "semi": [
        1,
        "never"
      ]
    }
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^2.2.11",
    "mongoose": "^4.7.0",
    "react": "^15.4.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-comments": "^1.9.1",
    "@kadira/storybook-addon-knobs": "^1.6.0",
    "@kadira/storybook-database-cloud": "^2.3.2",
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-react-app": "^1.0.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "cheerio": "^0.22.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.25.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "enzyme": "^2.6.0",
    "eslint": "3.8.1",
    "eslint-config-react-app": "^0.3.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "flow-bin": "^0.35.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "16.0.2",
    "jsdom": "^9.8.3",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "open-browser-webpack-plugin": "0.0.3",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.0",
    "react-dev-utils": "^0.3.0",
    "react-test-renderer": "^15.4.0",
    "recursive-readdir": "2.1.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-extension": "^1.0.0",
    "redux-devtools-log-monitor": "^1.1.1",
    "rimraf": "2.5.4",
    "selenium-standalone": "^5.8.0",
    "storybook-addon-specifications": "^1.0.15",
    "storybook-host": "^1.0.7",
    "storybook-readme": "^1.2.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "private": true,
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/jest/storybook.js",
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "cacheDirectory": "test/.cache",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "test/.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "globals": {},
    "notify": false,
    "resetMocks": false,
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/fbjs",
      "node_modules/enzyme",
      "cheerio",
      "htmlparser2",
      "underscore",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys"
    ]
  },
  "public": false
}
